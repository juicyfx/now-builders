{
  "version": 2,
  "builds": [
    { "src": "index.php", "use": "@juicyfx/php@canary" },
    { "src": "subdirectory/index.php", "use": "@juicyfx/php@canary" }
  ],
  "probes": [
    { "path": "/", "mustContain": "cow:RANDOMNESS_PLACEHOLDER" },
    { "path": "/subdirectory/", "mustContain": "yoda:RANDOMNESS_PLACEHOLDER" }
  ]
}
