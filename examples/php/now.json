    { "src": "cgi.php", "use": "@juicyfx/php@canary", "config": { "v": "7.3", "mode": "cgi" }},
    { "src": "cli.php", "use": "@juicyfx/php@canary", "config": { "v": "7.3", "mode": "cli" }},
    { "src": "fpm.php", "use": "@juicyfx/php@canary", "config": { "v": "7.3", "mode": "fpm" }},
